
@{
    ViewData["Title"] = "User Approval List";
}
@using DataTransferObject.Helpers;
@model SessionUserDTO;
@{
    string Page = "";
}
@if (ViewBag.UserStatus != null && (bool)ViewBag.UserStatus)
{
    Page = "STATUS OF UNIT CDR/COUNTERSIGNING AUTH";
     
}
else
{
    Page = "STATUS OF UNIT CDR/COUNTERSIGNING AUTH";
   
}
<div class="container">
    <div class="container-fluid">
    <div class="title-wrapper pt-30">
        <div class="row align-items-center">
            <div class="col-md-8">
                <div class="title">
                    <h2>
                        @Page
                    </h2>
                </div>
            </div>
            <!-- end col -->
            <div class="col-md-4">
                <div class="breadcrumb-wrapper">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item">
                                <a href="#0">Applications</a>
                            </li>
                            <li class="breadcrumb-item active" aria-current="page">
                                @Page
                            </li>
                        </ol>
                    </nav>
                </div>
            </div>
            <!-- end col -->
        </div>
        <!-- end row -->
    </div>

        <input type="hidden" id="Status" value="@ViewBag.UserStatus.ToString().ToLower()" />

    <div class="card mt-2">
       
        <div class="card-body">
            <div class="row">
                <div class="col-lg-12">
                    <div class="table-wrapper table-responsive">
                        <table class="table table-striped border border-purple table-hover" id="tblData">
                            <thead>
                                <tr>
                                    <th>Ser No</th>
                                    <th>Name</th>
                                    <th>Email Id</th>
                                    <th>Mobile No</th>
                                    <th>Army No</th>
                                    <th>Unit Name</th>
                                    <th>Appt</th>
                                    <th>Regt</th>
                                    <th>User Type</th>
                                    <th class="noExport">Status</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>

@section Scripts {
    <!-- CSS: Bootstrap 5 and DataTables -->
    <link href="~/lib/datatables.net-bs5/css/dataTables.bootstrap5.min.css" rel="stylesheet" />
    <link href="~/lib/datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css" rel="stylesheet" />

    <!-- JS: jQuery (must come before DataTables) -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <!-- JS: DataTables core -->
    <script src="~/lib/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables.net-bs5/js/dataTables.bootstrap5.min.js"></script>

    <!-- JS: DataTables Buttons extension -->
    <script src="~/lib/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/lib/datatables.net-buttons-bs5/js/buttons.bootstrap5.min.js"></script>
    
    <script src="~/devjs/getallusers.js"></script>
}

